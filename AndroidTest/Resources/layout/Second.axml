                        android:layout_height="wrap_content"
                        android:text="From a friend"
                        android:id="@+id/a" />
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Flyers"
                        android:id="@+id/b"
                        android:layout_below="@id/a" />
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Advertisement"
                        android:id="@+id/c"
                        android:layout_toRightOf="@id/b" />
