                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Just past by"
                        android:id="@+id/d"
                        android:layout_toRightOf="@id/b"
                        android:layout_below="@id/c" />
                </RadioGroup>
            </RelativeLayout>
